#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int numero1;
	int numero2;
	int resultado;

	scanf("%d",numero1);
	scanf("%d",numero2)	;

	resultado=numero1-numero2;

	printf("el resultado es %d",resultado);
}
